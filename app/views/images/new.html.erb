<%= form_for [current_user, @image], url: user_images_path(current_user), html: { multipart: true } do |form| %>
  <div class="form-group">
    <%= form.label :image_path %>
    <%= form.file_field :image_path %>
  </div>
  <div class="form-group">
    <%= form.label :tags %>
    <%= form.text_field :tags %>
  </div>
  <%= form.submit %>
<% end %>
